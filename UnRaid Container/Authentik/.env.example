AUTHENTIK_PROXY_HOST=https://auth.example.com
AUTHENTIK_PROXY_TOKEN=SOMETHINGSECURE #Gets defined by Authentik on Outpost creation

# ==================================
# RAC Container Settings
# ==================================
AUTHENTIK_RAC_HOST=https://auth.example.com
AUTHENTIK_RAC_TOKEN=SOMETHINGSECURE #Gets defined by Authentik on Outpost creation

# ==================================
# LDAP Container Settings
# ==================================
AUTHENTIK_LDAP_HOST=https://auth.example.com
AUTHENTIK_LDAP_TOKEN=SOMETHINGSECURE #Gets defined by Authentik on Outpost creation